{
  "name": "launchblocks",
  "version": "0.1.0",
  "description": "Spec-driven launchpad for AI-powered applications",
  "type": "module",
  "bin": {
    "launchblocks": "./dist/bin/launchblocks.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && rm -rf dist/templates && cp -r src/templates dist/templates",
    "dev": "tsup --watch",
    "start": "node dist/bin/launchblocks.js"
  },
  "keywords": [
    "ai",
    "llm",
    "scaffold",
    "cli",
    "supabase",
    "vercel",
    "vibe-coding",
    "launchblocks"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.3.7",
    "ora": "^9.3.0",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.17.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0"
  }
}
